map:RegelgevendDocDocumentDocumentRelatieVerandert
	rr:logicalTable [ rr:sqlQuery """
		SELECT 	(CASE Document.Numac WHEN 0 THEN Document.BSPagina ELSE Document.Numac END) AS ID,
				YEAR(CONVERT(date, Document.Datum)) AS datumDocumentJaar,
				MONTH(CONVERT(date, Document.Datum)) AS datumDocumentMaand,
				DAY(CONVERT(date, Document.Datum)) AS datumDocumentDag,
				(CASE  
					WHEN Document.WetgevingDocumentTypeId in (1000002, 1000003, 1000004, 1000007, 1000009, 1000010, 1000012, 1000018, 1000021, 1000029, 1000030, 1000032, 1000035, 1000038, 1000040, 1000041, 1000043, 1000047, 1000048) THEN 'BESLUIT'
