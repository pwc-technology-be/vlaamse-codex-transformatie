	];
	rr:predicateObjectMap [
		rr:predicate eli:amends; #Wijzigt
		rr:objectMap [
			rr:parentTriplesMap map:RegelgevendDocDocument; 
			rr:joinCondition [
				rr:child 'DocumentRelatie';
				rr:parent 'ID';
			]
		]
	].

